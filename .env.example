# Aurora environment example
AURORA_CONFIG_NAME=master_config_v1
AURORA_MODE=testnet
EXCHANGE_TESTNET=true
DRY_RUN=false
AURORA_OPS_TOKEN=please-change-me-32chars-minxxxxxxxx

# ACK/EXPIRE
AURORA_ACK_TTL_S=300
AURORA_ACK_SCAN_PERIOD_S=1

# CANCEL stub (enable only on testnet)
AURORA_CANCEL_STUB=false
AURORA_CANCEL_STUB_EVERY_TICKS=120

# Logging retention / rotation
AURORA_LOG_RETENTION_DAYS=7
AURORA_LOG_ROTATE_MAX_MB=200

# --- API/OPS ---
# If you run API locally and want to override host/port from YAML
# AURORA_API_HOST=127.0.0.1
# AURORA_API_PORT=8000
# OPS token alias (preferred is OPS_TOKEN); API also reads security.ops_token from YAML
# OPS_TOKEN=please-change-me-32chars-minxxxxxxxx

# --- Pretrade profile & gates ---
# er_before_slip | slip_before_er
# PRETRADE_ORDER_PROFILE=er_before_slip
# Minimum expected return (bps) to allow trade
# AURORA_PI_MIN_BPS=2.0
# Guard limits
# AURORA_SPREAD_BPS_LIMIT=80
# AURORA_LATENCY_MS_LIMIT=300
# Volatility guard (bps of std)
# AURORA_VOL_GUARD_STD_BPS=250

# --- Trap window ---
# AURORA_TRAP_WINDOW_S=2.0
# AURORA_TRAP_LEVELS=5
# AURORA_TRAP_Z_THRESHOLD=1.64
# AURORA_TRAP_CANCEL_PCTL=90

# --- Risk/sizing ---
# AURORA_MAX_CONCURRENT=3
# AURORA_SIZE_SCALE=0.15

# --- SPRT ---
# AURORA_SPRT_ENABLED=true
# AURORA_SPRT_ALPHA=0.05
# AURORA_SPRT_BETA=0.1
# AURORA_SPRT_A=2.0
# AURORA_SPRT_B=-2.0
# AURORA_SPRT_MAX_OBS=10

# --- Pushgateway (optional) ---
# PUSHGATEWAY_URL=http://localhost:9091

# --- Exchange API Keys (DO NOT COMMIT REAL KEYS) ---
# Binance API credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# Gate.io API credentials  
GATE_API_KEY=your_gate_api_key_here
GATE_API_SECRET=your_gate_api_secret_here

# Exchange settings
EXCHANGE_ID=binance  # or binanceusdm for futures
EXCHANGE_TESTNET=true
EXCHANGE_USE_FUTURES=false

# --- Session and logging ---
AURORA_SESSION_DIR=logs
AURORA_MAX_TICKS=1000

# --- HTTP timeouts ---
AURORA_HTTP_TIMEOUT_MS=1200
BINANCE_RECV_WINDOW=5000
